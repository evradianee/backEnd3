{
    "builds":[
        {
            "src" :"api/wsgi.py",
            "use" : "@vercel/python",
            "config" : {"maxLambdaSize" : "15mb" , "runtime":"python3.9"}
        },
        {
            "src" :"chatapp/wsgi.py",
            "use" : "@vercel/python",
            "config" : {"maxLambdaSize" : "15mb" , "runtime":"python3.9"}
        }
    ],
    "routes":[
        {
            "src" : "/(.*)",
            "methods": ["OPTIONS", "DELETE", "GET", "PATCH", "POST", "PUT"],
            "headers": {
                "Access-Control-Allow-Origin": "https://front-end6.vercel.app",
                "Access-Control-Allow-Headers": "Content-Type, Authorization",
                "Access-Control-Allow-Methods": "OPTIONS, DELETE, GET, PATCH, POST, PUT"
            },
            "dest" : "api/wsgi.py"
        },
        {
            "src" : "/(.*)",
            "methods": ["OPTIONS", "DELETE", "GET", "PATCH", "POST", "PUT"],
            "headers": {
                "Access-Control-Allow-Origin": "https://front-end6.vercel.app",
                "Access-Control-Allow-Headers": "Content-Type, Authorization",
                "Access-Control-Allow-Methods": "OPTIONS, DELETE, GET, PATCH, POST, PUT"
            },
            "dest" : "chatapp/wsgi.py"
        }
    ]
}